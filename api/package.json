{
    "name": "pay-baymax",
    "version": "0.1.0",
    "description": "A peer review system: an implementation of Pay-Pay dev challenge",
    "main": "server.js",
    "scripts": {
        "start": "node dist/server.js",
        "start:dev": "nodemon dist/server.js",
        "dev": "cross-env NODE_ENV=development nodemon server.js",
        "build:dev": "tsc --watch --preserveWatchOutput",
        "dev-ts": "concurrently \"npm:build:dev\" \"npm:start:dev\"",
        "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js --progress --profile --colors",
        "lint": "eslint ./app --fix",
        "test": "cross-env NODE_ENV=test jest --coverage --detectOpenHandles"
    },
    "keywords": [
        "Node.js",
        "Express",
        "REST API",
        "boilerplate"
    ],
    "dependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/bluebird": "^3.5.29",
        "@types/cors": "^2.8.6",
        "@types/express": "^4.17.2",
        "@types/lodash": "^4.14.149",
        "@types/node": "^13.7.0",
        "@types/uuid": "^3.4.7",
        "@types/validator": "^12.0.1",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "bunyan": "^1.8.12",
        "bunyan-middleware": "^1.0.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-session": "^1.17.0",
        "express-validator": "^6.3.0",
        "helmet": "^3.21.2",
        "lodash": "^4.17.15",
        "mysql2": "^2.1.0",
        "sequelize": "^5.21.3",
        "uuid": "^3.3.3"
    },
    "devDependencies": {
        "concurrently": "^5.1.0",
        "cross-env": "^6.0.3",
        "nodemon": "^2.0.1"
    }
}
